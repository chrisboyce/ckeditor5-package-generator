{
  "name": "<%= name %>",
  "version": "0.0.1",
  "description": "A plugin for CKEditor 5.",
  "keywords": [
    "ckeditor",
    "ckeditor5",
    "ckeditor 5",
    "ckeditor5-feature",
    "ckeditor5-plugin",
    "ckeditor5-dll"
  ],
  "main": "src/index.js",
  "license": "MIT",
  "dependencies": {
    "ckeditor5": ">=<%= version %>"
  },
  "devDependencies": {
    "@ckeditor/ckeditor5-dev-utils": "^<%= devToolsVersion %>",
    "@ckeditor/ckeditor5-editor-classic": ">=<%= version %>",
    "@ckeditor/ckeditor5-autoformat": ">=<%= version %>",
    "@ckeditor/ckeditor5-basic-styles": ">=<%= version %>",
    "@ckeditor/ckeditor5-block-quote": ">=<%= version %>",
    "@ckeditor/ckeditor5-image": ">=<%= version %>",
    "@ckeditor/ckeditor5-indent": ">=<%= version %>",
    "@ckeditor/ckeditor5-link": ">=<%= version %>",
    "@ckeditor/ckeditor5-list": ">=<%= version %>",
    "@ckeditor/ckeditor5-media-embed": ">=<%= version %>",
    "@ckeditor/ckeditor5-table": ">=<%= version %>",
    "@ckeditor/ckeditor5-essentials": ">=<%= version %>",
    "@ckeditor/ckeditor5-heading": ">=<%= version %>",
    "@ckeditor/ckeditor5-paragraph": ">=<%= version %>",
    "@ckeditor/ckeditor5-theme-lark": ">=<%= version %>"
  },
  "files": [
    "lang",
    "src",
    "theme",
    "build"
  ],
  "scripts": {
    "dll:build": "webpack --config webpack.dll.js",
    "lint": "eslint --quiet '**/*.js'",
    "pretest": "node scripts/generate-test-file.js",
    "test": "karma start karma.conf.js",
    "start": "webpack serve --config webpack.server.js",
    "stylelint": "stylelint --quiet --allow-empty-input 'src/**/*.css'"
  }
}
