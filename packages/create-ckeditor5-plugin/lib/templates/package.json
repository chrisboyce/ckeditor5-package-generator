{
  "name": "<%= name %>",
  "version": "0.0.1",
  "description": "A plugin for CKEditor 5.",
  "keywords": [
    "ckeditor",
    "ckeditor5",
    "ckeditor 5",
    "ckeditor5-feature",
    "ckeditor5-plugin",
    "ckeditor5-dll"
  ],
  "main": "src/index.js",
  "license": "MIT",
  "dependencies": {
    "ckeditor5": ">=<%= ckeditor5Version %>"
  },
  "devDependencies": {
    "@ckeditor/ckeditor5-package-tools": "<%= packageToolsVersion %>",
    "@ckeditor/ckeditor5-dev-utils": "^<%= devUtilsVersion %>",
    "@ckeditor/ckeditor5-editor-classic": ">=<%= ckeditor5Version %>",
    "@ckeditor/ckeditor5-autoformat": ">=<%= ckeditor5Version %>",
    "@ckeditor/ckeditor5-basic-styles": ">=<%= ckeditor5Version %>",
    "@ckeditor/ckeditor5-block-quote": ">=<%= ckeditor5Version %>",
    "@ckeditor/ckeditor5-image": ">=<%= ckeditor5Version %>",
    "@ckeditor/ckeditor5-indent": ">=<%= ckeditor5Version %>",
    "@ckeditor/ckeditor5-link": ">=<%= ckeditor5Version %>",
    "@ckeditor/ckeditor5-list": ">=<%= ckeditor5Version %>",
    "@ckeditor/ckeditor5-media-embed": ">=<%= ckeditor5Version %>",
    "@ckeditor/ckeditor5-table": ">=<%= ckeditor5Version %>",
    "@ckeditor/ckeditor5-essentials": ">=<%= ckeditor5Version %>",
    "@ckeditor/ckeditor5-heading": ">=<%= ckeditor5Version %>",
    "@ckeditor/ckeditor5-paragraph": ">=<%= ckeditor5Version %>",
    "@ckeditor/ckeditor5-theme-lark": ">=<%= ckeditor5Version %>"
  },
  "files": [
    "lang",
    "src",
    "theme",
    "build"
  ],
  "scripts": {
    "dll:build": "ckeditor5-package-tools dll:build",
    "lint": "eslint --quiet '**/*.js'",
    "start": "ckeditor5-package-tools start",
    "stylelint": "stylelint --quiet --allow-empty-input 'src/**/*.css'",
    "test": "ckeditor5-package-tools test"
  }
}
