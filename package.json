{
  "name": "@ckeditor/ckeditor5-package-generator",
  "version": "1.0.0-beta.4",
  "private": true,
  "keywords": [
    "ckeditor",
    "ckeditor5",
    "ckeditor 5",
    "package",
    "plugin",
    "generator",
    "maker",
    "wizard",
    "bootstrapper"
  ],
  "author": "CKSource (http://cksource.com/)",
  "license": "MIT",
  "homepage": "https://ckeditor.com/ckeditor-5",
  "devDependencies": {
    "@ckeditor/ckeditor5-dev-docs": "^29.0.0",
    "@ckeditor/ckeditor5-dev-env": "^29.0.0",
    "chalk": "^4.1.2",
    "eslint": "^7.32.0",
    "eslint-config-ckeditor5": "^3.1.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "mocha": "^9.1.1",
    "nyc": "^15.1.0",
    "strip-ansi": "^6.0.1"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=8.1.0"
  },
  "scripts": {
    "changelog": "node scripts/release/changelog.js",
    "coverage": "nyc --check-coverage --branches=100 --functions=100 --lines=100 --statements=100 --reporter=lcov --reporter=text-summary npm run test",
    "lint": "eslint --quiet \"**/*.js\"",
    "precommit": "lint-staged",
    "release:bump-version": "node scripts/release/bump-versions.js",
    "release:publish": "node scripts/release/publish.js",
    "test": "mocha 'packages/*/tests/**/*.js'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ckeditor/ckeditor5-package-generator.git"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --quiet"
    ]
  },
  "eslintIgnore": [
    "coverage/**",
    "packages/*/node_modules/**"
  ],
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "nohoist": [
      "**/husky"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
