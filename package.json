{
  "name": "@ckeditor/create-ckeditor5-plugin",
  "version": "0.0.1",
  "private": true,
  "author": "CKSource (http://cksource.com/)",
  "license": "GPL-2.0-or-later",
  "homepage": "https://ckeditor.com/ckeditor-5",
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-ckeditor5": "^3.1.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "mocha": "^9.1.1",
    "nyc": "^15.1.0"
  },
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "coverage": "nyc --reporter=lcov --reporter=text-summary npm run test",
    "lint": "eslint --quiet '**/*.js'",
    "precommit": "lint-staged",
    "test": "mocha packages/*/tests --recursive"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --quiet"
    ]
  },
  "eslintIgnore": [
    "coverage/**",
    "packages/*/node_modules/**"
  ],
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "nohoist": [
      "**/husky"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
